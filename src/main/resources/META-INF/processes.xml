<process-application
  xmlns="http://www.camunda.org/schema/1.0/ProcessApplication"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <process-archive name="bpm-student-day">
    <process-engine>default</process-engine>

    <resource>course-reservation-process.bpmn</resource>
    <resource>pricing.dmn</resource>
    <resource>payment_manually_secured.form</resource>
    <resource>course_evaluation.form</resource>

    <properties>
      <property name="isDeleteUponUndeploy">false</property>
      <property name="isScanForProcessDefinitions">true</property>
    </properties>
  </process-archive>

</process-application>
